<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryBackground" Color="#FFE6E6E6"/>
	<SolidColorBrush x:Key="SecondaryBackground" Color="#FFF7F9FB"/>

    <SolidColorBrush x:Key="BorderBrushDefault" Color="#FFCECECE"/>

    <SolidColorBrush x:Key="PrimaryText" Color="#333333"/>
	<SolidColorBrush x:Key="AccentColor" Color="#007ACC"/>

    <SolidColorBrush x:Key="IconColor" Color="#FF757575"/>
    <SolidColorBrush x:Key="IconHoverColor" Color="#E8EAED"/>

    <SolidColorBrush x:Key="SeparatorBackground" Color="#d0d0d0"/>

    <SolidColorBrush x:Key="ItemSelectedBackground" Color="#007ACC" Opacity="0.15" />

    <SolidColorBrush x:Key="BrowserTabBackground" Color="#FFECECEC" />

    <SolidColorBrush x:Key="BrowserTabSelect" Color="#FFF7F9FB" />
    
    

</ResourceDictionary>
